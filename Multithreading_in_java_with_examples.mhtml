#waldo-sticky-footer-wrapper iframe, #waldo-sticky-footer-wrapper div { margin-left: auto; margin-right: auto; }

.waldo-sticky-sidebar { position: fixed; top: 10px; z-index: 90; }

div[id^="waldo-tag-"] iframe[id^="primis_playerSekindoSPlayer"] { bottom: 80px !important; }

div[id^="waldo-tag-"] #flow_close_btn_iframe { bottom: 200px !important; }

#nm-ccpa-widget { width: 96%; margin-left: 2%; margin-right: 2%; right: unset; left: 0px !important; bottom: 0px !important; }

#nm-ccpa-widget { position: fixed; right: 20px; z-index: 9999; margin-bottom: 15px; box-shadow: rgba(0, 0, 0, 0.3) 0px 3px 36px 0px; background-color: rgb(255, 255, 255); border-radius: 10px; bottom: 20px; margin-top: 10px; font-size: 14px; }

.nm-ccpa-widget-header { background: rgb(23, 159, 132); color: rgb(255, 255, 255); padding: 20px; }

.nm-ccpa-widget-header .nm-ccpa-widget-main-title { margin-bottom: 0px; margin-top: 0px; font-size: 16px; font-weight: bold; }

.nm-ccpa-widget-body { padding: 20px; background: rgb(255, 255, 255); }

.nm-ccpa-widget-body label { font-size: 16px; }

.nm-ccpa-widget-submit-btn { margin-top: 15px; padding-left: 25px; }

.nm-ccpa-widget-submit-btn input { background: rgb(23, 159, 132); color: rgb(255, 255, 255); padding: 10px 20px; border-radius: 8px; border: 0px; outline: 0px; }

.nm-ccpa-widget-submit-btn input:hover, .nm-ccpa-widget-submit-btn input:focus { cursor: pointer; }

.nm-ccpa-widget-checkbox-label { font-size: 20px; }

.nm-ccpa-widget-header { position: relative; }

.nm-ccpa-widget-close-btn { position: absolute; right: 10px; top: 10px; }

.nm-ccpa-widget-close-btn a { color: rgb(255, 255, 255); font-size: 20px; text-decoration: none; }

.nm-ccpa-widget-message { padding-left: 25px; }

#waldo-tag-6038 { clear: both !important; }

div[class^="app_gdpr-"] a { color: rgb(65, 175, 187) !important; text-decoration: underline !important; }

#waldo-close-button { position: absolute; right: 0px; top: -24px; }

#waldo-close-button a { border: 1px solid rgba(0, 0, 0, 0.35); padding: 3px; font-size: 12px; color: rgb(255, 255, 255); font-weight: bold; background-color: rgb(119, 119, 119); }
